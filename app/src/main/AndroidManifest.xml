<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FavoriteActivity"
            android:label="@string/category_favorite" />
        <activity
            android:name=".WorkoutActivity"
            android:label="@string/category_workout" />
        <activity
            android:name=".WakeupActivity"
            android:label="@string/category_wakeup" />
        <activity
            android:name=".BedtimeActivity"
            android:label="@string/category_bedtime" />
        <activity
            android:name=".PlayNowActivity"
            android:label="Play Now">
        </activity>
    </application>

</manifest>